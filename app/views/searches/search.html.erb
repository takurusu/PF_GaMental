<h2>Results index</h2>
<table class="table table-hover table-inverse">
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= link_to user_path(user) do %>
              <%= user.name %>
            <% end %>
          </td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @games.each do |game| %>
        <tr>
          <td>
            <%= link_to game_path(game) do %>
              <%= game.title %>
            <% end %>
          </td>
          <td><%= game.text %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>